<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_order_process_status_inherit_form" model="ir.ui.view">
    <field name="name">sale.order.process.status.form</field>
    <field name="model">sale.order</field>
    <field name="inherit_id" ref="sale.view_order_form"/>
    <field name="arch" type="xml">
        <xpath expr="//field[@name='payment_term_id']" position="after">
            <field name="process_status_id" options="{'no_create': True, 'no_open': True}"/>
        </xpath>
    </field>
    </record>
</odoo>